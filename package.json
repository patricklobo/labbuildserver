{
  "name": "labbuildserver",
  "version": "1.0.6",
  "description": "Lab Build Server",
  "author": "Patrick Lobo",
  "license": "MIT",
  "scripts": {
    "public": "npx angular-http-server --path public -p 80",
    "dev": "nodemon index.js",
    "build": "rm -rf dist; babel src -d dist",
    "init": "npm run update; npm run build; npm start",
    "start": "node dist/index.js",
    "update": "git reset --hard origin/main && git pull && npm run build",
    "clearbd": "rm database.sqlite",
    "clearbdw": "del database.sqlite",
    "push": "git add . ; git commit -m lobo ; git push" 
  },
  "dependencies": {
    "angular-http-server": "^1.11.1",
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/compat-data": "^7.9.0",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6"
  }
}
